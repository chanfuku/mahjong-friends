# グループ
GET    /groups          controllers.GroupsController.lists()
GET    /groups/input    controllers.GroupsController.input()
GET    /groups/:id      controllers.GroupsController.show(id: Long)
POST   /groups/create   controllers.GroupsController.create()

# 収支
GET		/balance/input/:groupId	controllers.BalanceController.input(groupId: Long)
GET		/balance/histoty/:groupId	controllers.BalanceController.history(groupId: Long)
POST    /balance/create controllers.BalanceController.create()
